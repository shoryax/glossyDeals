generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Product {
  id        Int      @id @default(autoincrement())
  name      String
  price     Float?
  store     String
  category  String?
  imageUrl  String?
  link      String?
  scrapedAt DateTime @default(now())

  @@unique([name, store])
}
